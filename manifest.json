{
	"manifest_version": 2,
	"name": "needlles",
	"description": "Always with your Time",
	"version": "0.80",
	"chrome_url_overrides": {
		"newtab": "blank.html"
	},
	"icons": {
		"16": "icon.png",
		"128": "icon.png"
	},
	"permissions": [
		    "http://*/*",
		    "https://*/*"
	],
    "externally_connectable": {
            "matches": ["https://oauth.io/*"]
    },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}